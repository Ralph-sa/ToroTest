pipeline {
  agent any
  stages {
    stage('1') {
      steps {
        echo 'HelloWorld'
      }
    }

    stage('2') {
      steps {
        sleep 2
        echo '2'
      }
    }

    stage('3') {
      steps {
        sleep 2
      }
    }

    stage('4') {
      steps {
        sleep 2
        echo '4'
      }
    }

  }
}